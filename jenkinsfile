pipeline {
	agent any

	stages {

	     stage ('Compile') {
		steps {
		  withMaven(maven : 'maven') {
			sh 'mvn clean compile'

			}
		  }

		}

stage ('Executing Test Cases') {
		steps {
		  withMaven(maven : 'maven_3_5_0') {
			sh 'mvn test'

			}
		  }

		}

stage ('Deployment') {
		steps {
		  withMaven(maven : 'maven_3_5_0') {
			sh 'mvn deploy'

			}
		  }

		}
	}


}
